{
  "name": "q-municate-web",
  "version": "2.0.0-alpha",
  "description": "Q-municate Web chat application",
  "homepage": "http://q-municate.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:QuickBlox/q-municate-web.git"
  },
  "license": "(Apache-2.0)",
  "private": true,
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=development npm run server",
    "server": "webpack-dev-server --config webpack.config.js --progress",
    "build": "npm run build:prod",
    "build:prod": "cross-env NODE_ENV=production npm run bundle",
    "clean": "rimraf dist",
    "copy:assets": "cpy audio/ workers/ \"../dist\" --cwd=src/assets --parents",
    "prebundle": "npm-run-all lint clean copy:assets",
    "bundle": "webpack --config webpack.config.js --progress",
    "postbundle": "npm-run-all --parallel compile:*",
    "compile:css": "npm-run-all cssmin:* banner:css rev:css",
    "compile:js": "npm-run-all minify:* banner:js rev:js",
    "compile:img": "npm-run-all --parallel imagemin:*",
    "cssmin:build": "cleancss app/.tmp/build.css -o app/.tmp/build.min.css -O2",
    "cssmin:vendor": "cleancss app/.tmp/vendor.css -o app/.tmp/vendor.min.css -O2",
    "minify:build": "uglifyjs app/.tmp/build.js -o app/.tmp/build.min.js -m",
    "minify:vendor": "uglifyjs app/.tmp/vendor.js -o app/.tmp/vendor.min.js -m",
    "banner:css": "headr app/.tmp/build.min.css -o=app/.tmp/build.min.css --version --homepage --license",
    "banner:js": "headr app/.tmp/build.min.js -o=app/.tmp/build.min.js --version --homepage --license",
    "rev:css": "hashmark -s -l 8 app/.tmp/*.min.css dist/styles/{hash}-{base}",
    "rev:js": "hashmark -s -l 8 app/.tmp/*.min.js dist/scripts/{hash}-{base}",
    "imagemin:parent": "imagemin app/images/* --out-dir=dist/images",
    "imagemin:smiles": "imagemin app/images/smiles-group/* --out-dir=dist/images/smiles-group",
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint src/app/styles",
    "lint:js": "eslint src/app/scripts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": "stylelint",
    "*.js": "eslint"
  },
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=6.4.1"
  },
  "dependencies": {
    "backbone": "^1.4.0",
    "balloon-css": "^1.0.1",
    "blueimp-load-image": "^2.22.0",
    "crypto-js": "^3.1.9-1",
    "firebase": "^6.2.4",
    "jquery": "^2.2.4",
    "jquery-mousewheel": "^3.1.13",
    "jquery.nicescroll": "^3.7.6",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "media-recorder-js": "^2.1.0",
    "perfect-scrollbar": "^0.8.1",
    "quickblox": "^2.12.2",
    "timeago": "^1.6.7",
    "underscore": "^1.9.1",
    "web-notifications": "^1.1.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-css-cli": "^4.3.0",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-backbone": "^2.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-optimize-regex": "^1.1.6",
    "exports-loader": "^0.7.0",
    "file-loader": "^4.0.0",
    "hashmark": "^5.0.0",
    "headr": "0.0.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "imagemin-cli": "^5.0.0",
    "lint-staged": "^9.1.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "uglify-es": "^3.3.9",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
