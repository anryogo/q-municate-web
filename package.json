{
  "name": "q-municate-web",
  "version": "2.0.0-alpha",
  "description": "Q-municate Web chat application",
  "homepage": "http://q-municate.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:QuickBlox/q-municate-web.git"
  },
  "license": "(Apache-2.0)",
  "private": true,
  "scripts": {
    "start": "npm run server",
    "server": "webpack-dev-server --config configs/webpack.dev.js --progress --open",
    "build": "npm-run-all lint clean compile copy:assets",
    "clean": "rimraf dist",
    "compile": "webpack --config configs/webpack.prod.js --progress",
    "copy:assets": "cpy audio/ images/ workers/ \"../../dist\" --cwd=src/assets --parents",
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint src/app/styles",
    "lint:js": "eslint src/app/scripts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": "stylelint",
    "*.js": "eslint"
  },
  "engines": {
    "node": ">=12.18.1",
    "npm": ">=6.14.5"
  },
  "dependencies": {
    "backbone": "1.4.0",
    "balloon-css": "^1.1.0",
    "blueimp-load-image": "^5.13.0",
    "crypto-js": "^4.0.0",
    "firebase": "7.15.4",
    "jquery": "2.2.4",
    "jquery-mousewheel": "3.1.13",
    "jquery.nicescroll": "3.7.6",
    "malihu-custom-scrollbar-plugin": "3.1.5",
    "media-recorder-js": "^2.1.0",
    "perfect-scrollbar": "0.8.1",
    "quickblox": "2.12.3",
    "timeago": "^1.6.7",
    "underscore": "^1.10.2",
    "web-notifications": "^1.1.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "autoprefixer": "^9.8.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cpy-cli": "^3.1.1",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-backbone": "^2.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "exports-loader": "^1.0.1",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "imagemin-webpack": "^5.1.1",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
